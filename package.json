{
  "name": "contoso-air-dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server.js --production",
    "local:build": "webpack --env.NODE_ENV=development --mode=development",
    "build": "exit 0",
    "unit-test": "jest",
    "int-test": "jest --testRegex=.*\\.ispec\\.js$",
