    "all-test:watch": "jest --watch --testRegex=.*\\.i?spec\\.js$"
  },
  "jest": {
    "testResultsProcessor": "jest-junit-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "cobertura",
      "html"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-datepicker": "^1.9.0",
    "debug": "~2.6.9",
    "express": "~4.17.1",
    "express-flash": "0.0.2",
    "express-handlebars": "^5.3.4",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "i18n": "^0.13.4",
    "moment": "^2.29.1",
    "mongoose": "^6.0.14",
    "morgan": "~1.10.0",
    "npm": "^7.20.6",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "popper.js": "^2.9.3",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
